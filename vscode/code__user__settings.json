        { "before": ["F", "¨"], "after": ["F", "]"] },
        { "before": ["F", "Å"], "after": ["F", "{"] },
        { "before": ["F", "^"], "after": ["F", "}"] },

        { "before": ["t", "å"], "after": ["t", "["] },
        { "before": ["t", "¨"], "after": ["t", "]"] },
        { "before": ["t", "Å"], "after": ["t", "{"] },
        { "before": ["t", "^"], "after": ["t", "}"] },
        { "before": ["T", "å"], "after": ["T", "["] },
