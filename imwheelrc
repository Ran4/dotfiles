# Speed up scrolling in Zathura
"^zathura$"
    None, Up, Button4, 4
    None, Down, Button5, 4
# Speed up scrolling in Chrome
".*-chrome*"
    None,      Up,   Button4, 3
    None,      Down, Button5, 3
    Control_L, Up,   Control_L|Button4
    Control_L, Down, Control_L|Button5
    Shift_L,   Up,   Shift_L|Button4
    Shift_L,   Down, Shift_L|Button5
